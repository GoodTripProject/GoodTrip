<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/mobile_navigation"
  app:startDestination="@+id/navigation_feed">

  <fragment
    android:id="@+id/navigation_map"
    android:name="ru.hse.goodtrip.ui.map.MapsFragment"
    tools:layout="@layout/fragment_maps" />

  <fragment
    android:id="@+id/navigation_profile"
    android:name="ru.hse.goodtrip.ui.profile.ProfileFragment"
    tools:layout="@layout/fragment_profile" />

  <fragment
    android:id="@+id/navigation_feed"
    android:name="ru.hse.goodtrip.ui.feed.FeedFragment"
    tools:layout="@layout/fragment_feed" />

  <fragment
    android:id="@+id/navigation_places"
    android:name="ru.hse.goodtrip.ui.places.PlacesFragment"
    tools:layout="@layout/fragment_places" />

  <fragment
    android:id="@+id/navigation_login"
    android:name="ru.hse.goodtrip.ui.authentication.LoginFragment"
    tools:layout="@layout/fragment_login" />

  <fragment
    android:id="@+id/navigation_sign_up"
    android:name="ru.hse.goodtrip.ui.authentication.SignUpFragment"
    tools:layout="@layout/fragment_sign_up" />

  <fragment
    android:id="@+id/navigation_plan_trip"
    android:name="ru.hse.goodtrip.ui.trips.PlanTripFragment"
    tools:layout="@layout/fragment_plan_trip" />
</navigation>